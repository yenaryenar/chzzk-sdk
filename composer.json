{
    "name": "cherryred5959/chzzk-sdk",
    "version": "0.0.1",
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2"
    },
    "autoload": {
        "psr-4": {
            "Cherryred5959\\ChzzkApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cherryred5959\\ChzzkApi\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/Dockerfile",
            "/docker-compose.yml",
            "/examples",
            "/tests",
            "/*.md"
        ]
    }
}
